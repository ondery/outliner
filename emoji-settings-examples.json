// TypeScript Outliner - Emoji Customization Examples
// Copy these settings to your VS Code settings.json

{
  // Enable/disable emoji icons in labels
  "tsOutlineEnhancer.showIconsInLabel": true,

  // Show visibility text in brackets (optional)
  "tsOutlineEnhancer.showVisibilityInLabel": false,

  // Visibility Emojis - Customize freely!
  "tsOutlineEnhancer.emojis.public": "🌐", // Default: Global/World
  "tsOutlineEnhancer.emojis.private": "🔒", // Default: Lock
  "tsOutlineEnhancer.emojis.protected": "🛡️", // Default: Shield

  // Modifier Emojis - Mix and match!
  "tsOutlineEnhancer.emojis.static": "📌", // Default: Pin
  "tsOutlineEnhancer.emojis.readonly": "📖", // Default: Book
  "tsOutlineEnhancer.emojis.abstract": "🎭", // Default: Theater Mask
  "tsOutlineEnhancer.emojis.async": "⚡" // Default: Lightning

  // Alternative Color-themed Examples:
  // "tsOutlineEnhancer.emojis.public": "🟢",    // Green circle
  // "tsOutlineEnhancer.emojis.private": "🔴",   // Red circle
  // "tsOutlineEnhancer.emojis.protected": "🟡", // Yellow circle

  // Alternative Style Examples:
  // "tsOutlineEnhancer.emojis.public": "✅",     // Check mark
  // "tsOutlineEnhancer.emojis.private": "❌",    // X mark
  // "tsOutlineEnhancer.emojis.protected": "⚠️", // Warning

  // Fun Examples:
  // "tsOutlineEnhancer.emojis.static": "🏗️",    // Construction
  // "tsOutlineEnhancer.emojis.readonly": "👀",   // Eyes
  // "tsOutlineEnhancer.emojis.abstract": "💭",   // Thought bubble
  // "tsOutlineEnhancer.emojis.async": "🚀",     // Rocket

  // Pro tip: You can use any emoji or even regular text!
  // "tsOutlineEnhancer.emojis.private": "[P]",   // Text instead of emoji
  // "tsOutlineEnhancer.emojis.async": "🔄🔄",    // Multiple emojis
}
