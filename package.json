{
  "name": "typescript-outline-enhancer",
  "author": "Ã–ndery",
  "displayName": "TS OUTLINER",
  "description": "Enhances TypeScript outline with customizable emoji icons for visibility and modifiers",
  "version": "0.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ondery/outliner.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typescript-outline-enhancer.refresh",
        "title": "Refresh TS Outliner",
        "icon": "$(refresh)"
      },
      {
        "command": "tsOutlineEnhancer.goToLine",
        "title": "Go to Line"
      },
      {
        "command": "tsOutlineEnhancer.openEmojiSettings",
        "title": "Open Emoji Settings",
        "category": "TS Outliner"
      },
      {
        "command": "tsOutlineEnhancer.sortByPosition",
        "title": "Sort By: Position",
        "category": "TS Outliner"
      },
      {
        "command": "tsOutlineEnhancer.sortByName",
        "title": "Sort By: Name",
        "category": "TS Outliner"
      },
      {
        "command": "tsOutlineEnhancer.sortByCategory",
        "title": "Sort By: Category",
        "category": "TS Outliner"
      },
      {
        "command": "tsOutlineEnhancer.sortByPositionChecked",
        "title": "$(check) Sort By: Position",
        "category": "TS Outliner"
      },
      {
        "command": "tsOutlineEnhancer.sortByNameChecked",
        "title": "$(check) Sort By: Name",
        "category": "TS Outliner"
      },
      {
        "command": "tsOutlineEnhancer.sortByCategoryChecked",
        "title": "$(check) Sort By: Category",
        "category": "TS Outliner"
      }
    ],
    "views": {
      "TS OUTLINER": [
        {
          "id": "tsOutlineEnhancer",
          "name": "TypeScript Enhanced Outline",
          "when": "resourceLangId == typescript || resourceLangId == typescriptreact",
          "icon": "$(symbol-class)"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "typescript-outline-enhancer.refresh",
          "when": "view == tsOutlineEnhancer",
          "group": "navigation"
        },
        {
          "command": "tsOutlineEnhancer.sortByPosition",
          "when": "view == tsOutlineEnhancer && !tsOutlineEnhancer.sortMode.position",
          "group": "1_sort@1"
        },
        {
          "command": "tsOutlineEnhancer.sortByPositionChecked",
          "when": "view == tsOutlineEnhancer && tsOutlineEnhancer.sortMode.position",
          "group": "1_sort@1"
        },
        {
          "command": "tsOutlineEnhancer.sortByName",
          "when": "view == tsOutlineEnhancer && !tsOutlineEnhancer.sortMode.name",
          "group": "1_sort@2"
        },
        {
          "command": "tsOutlineEnhancer.sortByNameChecked",
          "when": "view == tsOutlineEnhancer && tsOutlineEnhancer.sortMode.name",
          "group": "1_sort@2"
        },
        {
          "command": "tsOutlineEnhancer.sortByCategory",
          "when": "view == tsOutlineEnhancer && !tsOutlineEnhancer.sortMode.category",
          "group": "1_sort@3"
        },
        {
          "command": "tsOutlineEnhancer.sortByCategoryChecked",
          "when": "view == tsOutlineEnhancer && tsOutlineEnhancer.sortMode.category",
          "group": "1_sort@3"
        }
      ]
    },
